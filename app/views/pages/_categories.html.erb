<div class="container categories mt-5">
    <div class="row">
      <% @services.pluck(:category).uniq.each do |category| %>
        <div class="col-md-4 mb-4">
          <div class="category-card">
            <h3><%= category %></h3>
          </div>
        </div>
      <% end %>
  </div>
</div>
