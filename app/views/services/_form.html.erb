  <%= simple_form_for(@service) do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.input :category, collection: ["content-writing", "landing-page", "back-end", "front-end", "e-commerce", "mobile-development", "graphic-design", "digital-marketing", "social-media", "video-creation"] %>
  <%= f.input :delivery_time %>
  <%= f.input :images, as: :file, input_html: { multiple: true } %>
  <%= f.submit %>
<% end %>
